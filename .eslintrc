{
  "env": {
    "es6": true
  },
  "extends": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "no-multiple-empty-lines": 2,
    "prettier/prettier": ["error"]
  }
}
